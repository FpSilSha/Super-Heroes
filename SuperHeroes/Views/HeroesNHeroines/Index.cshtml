@model IEnumerable<SuperHeroes.Models.HeroesNHeroines>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SuperName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Alter_Ego_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Primary_SuperAbility)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Secondary_SuperAbility)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CatchPhrase)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SuperName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Alter_Ego_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Primary_SuperAbility)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Secondary_SuperAbility)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CatchPhrase)
        </td>
        <td>
            @Html.ActionLink("Edit", "Update", new { id=item.Id }) |
            @Html.ActionLink("Details", "Read", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
